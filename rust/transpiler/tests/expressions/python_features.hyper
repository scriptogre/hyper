items: list
data: dict
value: int

---

# Walrus operator
if (n := len(items)) > 0:
    <span>Found {n} items</span>
end

# Lambda (assigned to variable first)
sorter = lambda x: x.lower()
sorted_items = sorted(items, key=sorter)
<span>{sorted_items}</span>

# Unpacking in expression
first, *rest = items
<span>First: {first}, Rest: {rest}</span>

# Dictionary merge (Python 3.9+)
merged = {**data, "extra": "value"}
<span>{merged}</span>

# Conditional expression (ternary)
<span>{value if value > 0 else -value}</span>

# Nested ternary
<span>{"positive" if value > 0 else "zero" if value == 0 else "negative"}</span>

# F-string style expressions
<span>{value=}</span>
<span>{items!r}</span>

# Star unpacking in function call
args = [1, 2, 3]
<span>{max(*args)}</span>

# Keyword unpacking
kwargs = {"sep": ", ", "end": "!"}
result = "hello"
<span>{result}</span>
